{
  "name": "aarch64-none-elf-gcc-action",
  "version": "1.10.1",
  "private": true,
  "description": "GitHub Action to install aarch64-none-elf-gcc",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --config config/prettierrc.json --write **/*.ts",
    "format-check": "prettier --config config/prettierrc.json --check **/*.ts",
    "lint": "eslint --config config/eslintrc.json src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest --config config/jest.config.js",
    "test:urls": "jest --config config/jest.config.js -t \"Check links work\"",
    "all": "npm run build && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lawkai-vivo/aarch64-none-elf-gcc-action.git"
  },
  "keywords": [
    "actions",
    "gcc",
    "arm",
    "aarch64-none-elf-gcc"
  ],
  "author": "Kai Law",
  "license": "MIT",
  "dependencies": {
    "@actions/cache": "^4.0.3",
    "@actions/core": "^1.10.1",
    "@actions/tool-cache": "^2.0.1",
    "md5-file": "^5.0.0",
    "semver": "^7.5.2",
    "sha256-file": "^1.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.9.36",
    "@types/jest": "^29.2.0",
    "@types/node": "^18.19.121",
    "@types/rimraf": "^3.0.2",
    "@types/semver": "^7.3.12",
    "@types/sha256-file": "^1.0.3",
    "@typescript-eslint/parser": "^5.40.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.6.0",
    "eslint-plugin-github": "^4.3.7",
    "eslint-plugin-jest": "^27.1.3",
    "jest": "^29.2.1",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.5"
  }
}
